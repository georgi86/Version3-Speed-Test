[Manual]
###############################################################################
#MANUAL MACHINE CONFIGURATION PARAMETERS
# AutoscalePercentage
#	Enter a number between 0 and 100. This means that the stage will
#	travel that percentage of the distance to the target feature before
#    zooming in.
#
#  FocusMinBin
#	The minimum number of raw data values needed for each z position
#	bin before the focus position is calculated
#	Default 3
#
#  FocusDepthOfField
#	The multipler used to determine the depth of field (Z range) of the
#	graphical interface for a manual focus.  This value is multiplied
#	by the STEP SIZE in MICRONS from the focus.cfg table entry
#	Default 20.0
#
#  FocusXContrastOnly
#	Only use the horizontal component of image contrast for the focus
#	calculation instead of both the horizontal and vertical.
#	This may yield more repeatable results, except scenes with only
#	horizontal lines will not focus
#	Default 0 (off)
#
#  FocusPeakValuesOnly
#	This parameter forces the focus routine to use ONLY the peak
#	contrast values measured at a given Z position, the idea being
#	that either vibration or motion will lower the contrast, so by
#	saving only peak values we are statistically reducing the influence
#	of vibration and motion
#	Default 1 (on)
#
#  FocusSlowMotionOnly
#	Do not collect contrast data if the user is moving the Z slide
#	faster than a pre-defined velocity derived from the value below
#	(from manual_focus_motion_limit)
#	Default 0 (off)
#
#  FocusPixelSkip
#	Number of pixels to skip while calculating contrast over the
#	video patch (min 1 = best repeatability, max 10 = fastest)
#	Default 1
#
#  FocusForceToBest
#	Force the user to move the Z slide to the found focus position
#	and then declare the focus completed
#	Default 1 (on)
#
#  FocusMotionLimit
#	When the above parameter is set to ON, do not collect contrast
#	data if the Z slide is moved more than this amount between
#	two video frames (inches).
#	Default 0.002
#
#  CalibrationDelay
#	The machine waits this amount of time before taking a data point
#	after the person stops moving the stage manually.
#	Default 2.0 seconds
#
#  GUIFillColor
#	Color filled in for good focus data
#	default = 12 (blue)
#
#  GUIFoundColor
#	Color of the large bar indicating where the good focus is (cross hatch)
#    default = 10 (lime)
#
#  GUIMissedColor
#	Color filled in for missed or bad focus data
#    default = 9 (red)
#
#  Color Index
#    0 black     4 navy       8 silver    12 blue
#    1 maroon    5 purple     9 red       13 fuscia
#    2 green     6 teal      10 lime      14 aqua
#    3 olive     7 grey      11 yellow    15 white
#
#  FocusZTolerance
#	If ForceFocusToBest is turned on then the step size is multiplied by this
#    to tolerance when we are positioned close enough to the found z value.
#	Default .30
#
###############################################################################
AUTOSCALE_PERCENTAGE=75
FOCUS_MIN_BIN=3
FOCUS_DEPTH_OF_FIELD=20.000000
FOCUS_X_CONTRAST_ONLY=0
FOCUS_PEAK_VALUES_ONLY=1
FOCUS_SLOW_MOTION_ONLY=0
FOCUS_PIXEL_SKIP=1
FOCUS_FORCE_TO_BEST=1
FOCUS_MOTION_LIMIT=0.002000
CALIBRATION_DELAY=2.000000
GUI_FILL_COLOR=12
GUI_FOUND_COLOR=10
GUI_MISSED_COLOR=9
FOCUS_Z_TOLERANCE=0.300000


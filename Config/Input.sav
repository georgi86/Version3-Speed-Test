[Strong Edge]
###############################################################################
#STRONG EDGE CONFIGURATION PARAMETERS
#  ContrastThreshold
#    ( 0.0 to 100.0 percent )
#  MinimumContrastLevel
#    This is a contrast value below which the system lights will
#    be TURNED UP until the contrast is above this value.
#  MaximumContrastLevel
#    This is a contrast value above which the system lights will
#    be TURNED DOWN until the contrast is below this value.
#  FrameAverages
#    number of video frames to average together
#    for each edge measurement (1 - fast, 10 - more repeatable)
#  AutoRemeasure
#    0 - Do Not remeasure the edge during Measure Mind auto edge
#    1 - Remeasure the edge and tweak the stage each time
#  SearchAhead
#    0 - Do not move the stage to search ahead for the edge when in
#    automatic playback using directional edge finding
#    10-100 A percentage of the EdgeFinder box width to move the stage by
#  SearchRetries
#    0   Do not move the stage to search ahead for the edge
#    1..10 Number of times to move the stage looking for an edge
#  TargetSize
#    Target size in pixels.
#  MinimumTargetSize
#    Minimum target size in pixels.
#  MaximumTargetSize
#    Maximum target size in pixels.
#  LightTooHigh
#    0 - Do not fail an edge measurement when the light is too high
#    1 - Fail the edge measurement when the camera is in saturation
#    2 - Fail only when backlight is on
#  CheckLightLevel
#    0 - Do not change light levels when automatically taking an edge
#    1 - Change the light level to a midrange value during MX playback
#    2 - Change the light level to a midrange value all the time
###############################################################################
EDGE_CONTRAST_THRESHOLD=1.000000
EDGE_MIN_CONTRAST_LEVEL=20
EDGE_MAX_CONTRAST_LEVEL=70
EDGE_FRAME_AVERAGES=2
EDGE_AUTO_REMEASURE=1
EDGE_SEARCH_AHEAD=0
EDGE_SEARCH_RETRYS=0
EDGE_TARGET_SIZE=30
EDGE_MIN_TARGET_SIZE=20
EDGE_MAX_TARGET_SIZE=300
EDGE_LIGHT_2_HIGH=0
EDGE_CHECK_LIGHT_LEVEL=0

[Weak Edge]
###############################################################################
#WEAK_EDGE CONFIGURATION PARAMETERS
#  Bounds
#    Pixel distance from edge to end of patch (6 to 60).
#  Extent
#    Pixel distance from start of one patch to the next (4 to 100).
#  MinQuality
#    Threshold, used in finding edges (4 to 100)
#  CalibrationMinQuality
#    Threshold used in finding edges (4 to 100) ONLY for non-linear calibration
#  StandardDeviation
#    Pixel error allowable distance (in tenths) (1 to 100)
#  Contrast
#    Target contrast level for edges (1 to 256)
#  DrawArrows
#    0 - Do not draw arrows
#    1 - Draw arrows
#  ArrowSize
#    
#  BoundsSize
#    
#  CarryParameters
#    0 - Do not carry parameters
#    1 - Carry parameters
#    
#  PointEliminationPercent
#		Percent of points to eliminate should be in 0-50 range
#    0  - No elimination (Default)
#    
#  TestCalMinQuality
#    Threshold used to determine whether a refocus is necessary (4 - 100)
#    ONLY for non-linear calibration
#    
#  SECOND_EVALUATION
#    0 - Do not perform (Default)
#    1 - Enable second evaluation
#    2 - Enable second evaluation for back light only
#    
###############################################################################
DEFAULT_BOUNDS=30
DEFAULT_EXTENT=4
DEFAULT_MINQUAL=17
CALIBRATE_MINQUAL=50
DEFAULT_STD=10
DEFAULT_TARCONT=0
DRAW_ARROWS=1
ARROW_SIZE=0
BOUND_SIZE=2
CARRY_PARAMETERS=0
POINT_ELIMINATION_PERCENT=0
TEST_CAL_MINQUAL=70

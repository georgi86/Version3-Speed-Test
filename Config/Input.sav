[Metrology]
###############################################################################
#METROLOGY CONFIGURATION PARAMETERS
#  AltNegativeDevation
#    0 - Do not use alternate (mathematical) method of calculating a negative
#        deviation.
#    1 - Use alternate (mathematical) method of calculating a negative
#        deviation.
#  UseISOTolerance
#    0 - Do not use ISO Tolerance
#    1 - Use ISO Tolerance
#  BadValueDigit
#    0..9 - Digit to use when a bad value is encountered.
#      -1 - Output the value as is
#  AllowFiveSigDigitsInMM
#    0 - Do not allow five significant digits
#    1 - Allow five significant digits
#
#  WIDTH_MIN_FEAT_MULTIPLIER
#		Default: 5.25 x pixel size
#  DIAMETER_MIN_FEAT_MULTIPLIER
#		Default: 19.1 x pixel size
#
###############################################################################
ALT_NEGATIVE_DEVIATION=0
USE_ISO_TOLERANCING=0
BAD_VALUE_DIGIT=9
FIVE_SIG_DIGITS_MILLIMETERS=1
WIDTH_MIN_FEAT_MULTIPLIER=5.250000
DIAMETER_MIN_FEAT_MULTIPLIER=19.100000

[Graphics]
###############################################################################
#GRAPHICS CONFIGURATION PARAMETERS
#  
#  PenSize
#    For best results use odd number pen sizes. (1 is default)
#  Target
#    0 - Cross Hair      8 - Chart Combination        16 - Minimum Weak Edge
#    1 - Microgage       9 - Multiple                 17 - Laser Focus
#    2 - Box            10 - Strong Edge              18 - Laser Line Scan
#    3 - Circle         11 - not used                 19 - Laser Arc Scan
#    4 - Double Circle  12 - Focus                    20 - Edge Trace
#    5 - Protractor     13 - Average Weak Edge        21 - Centroid
#    6 - Chart Radius   14 - Weak Edge FeatureFinder  22 - Advanced Focus
#    7 - Chart Grid     15 - Maximum Weak Edge
#  TargetColor
#    0 - Black          6 - Brown                 11 - Light Cyan
#    1 - Blue           7 - Grey                  12 - Light Red
#    2 - Green          8 - Dark Grey             13 - Light Magenta
#    3 - Cyan           9 - Light Blue            14 - Yellow
#    4 - Red           10 - Light Green           15 - White
#    5 - Magenta
#  TargetXSpacing
#    Target spacing in pixels.
#  TargetYSpacing
#    Target spacing in pixels.
#  YellowPercent
#    Percentage of tolerance to start displaying the color yellow.
#  VideoKeyOverride
#    -1 - do not override
#    0  - key on black
#    3  - key on brown in 256 color mode (or try 19)
#    14 - key on brown in 16 color mode
#  DisplayOpticalMag
#    0 - Do not display the optical magnification.
#    1 - Display the optical magnification.
#  DisplayTargetSize
#    0 - Do not display the target size.
#    1 - Display the target size.
#  DisplaySetupInstructions
#    0 - Do not display the setup instructions.
#    1 - Display the setup instructions.
#  ModelMinimumZoom
#    Number of pixels mouse must move before a rubberband zoom box is displayed.
#  GlobalPrecision
#    0 - Don't use precision globally (display as programmed)
#    1 - Use precision globally
#  DisplayHeader
#    0 - Do not display the header.
#    1 - Display the header.
#  User Input default cursor location
#    0 - Default cursor location to numeric value edit box.
#    1 - Default cursor location to text edit box.
# 
#   DISPLAY_ABS_Z
#      When turned on (1) the ABS value will be displayed in the lower left corner of the image window at all times.
#      Default value is off (0), not displayed
# 
#  GRAPHICS_CFG_DLG_XPOS
#    X (left) Edit Config Dialog window position
#  GRAPHICS_CFG_DLG_YPOS
#    Y (top) Edit Config Dialog window position
#  GRAPHICS_CFG_DLG_WIDTH
#    Width of Edit Config Dialog window
#  GRAPHICS_CFG_DLG_HEIGHT
#    HEIGHT of Edit Config Dialog window
# 
#  UI5_BIG_3D_BTNS
#    0 - Win 7 style buttons
#    1 - 3D buttons (default)
# 
###############################################################################
PEN_SIZE_DFLT=1
TARGET_DFLT=11
TARGET_COLOR_DFLT=10
TARGET_X_SPACING=50
TARGET_Y_SPACING=50
YELLOW_PERCENT=75
VIDEO_KEY_OVERRIDE=-1
DISPLAY_OPTICAL_MAG=0
DISPLAY_TARGET_SIZE=1
RUN_DISPLAY_SETUP=0
MODEL_MIN_ZOOM=25
GLOBAL_PRECISION=1
RUN_DISPLAY_HEADER=0
USER_INPUT_DEFAULT_CURSOR_LOCATION=0
DISPLAY_ABS_Z=0
UI5_BIG_3D_BTNS=1

